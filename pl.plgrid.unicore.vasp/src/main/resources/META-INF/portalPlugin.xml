<?xml version="1.0" encoding="UTF-8"?>
<plugin>
    <extensions>
        <menuExtension>
            <id>pl.plgrid.unicore.vasp.ui</id>
            <menuPath>navigation:main</menuPath>

            <name>Run VASP</name>
            <description>Prepare VASP Simulation</description>
            <!--<icon>vfsaction/addWorkflow</icon>-->

            <category>/ApplicationVASP</category>
            <after>Grid</after>
            <!--<before></before>-->

            <commandDescriptor>
                <commandId>eu.unicore.portal.ui.views.switchTo</commandId>
                <parameter>
                    <key>view.class</key>
                    <value class="string">pl.plgrid.unicore.vasp.MainPortalView</value>
                </parameter>
            </commandDescriptor>
        </menuExtension>


        <MessageSourceExtension>
            <id>pl.plgrid.unicore.vasp.messages</id>
            <location>pl.plgrid.unicore.vasp.i18n.Messages</location>
        </MessageSourceExtension>
    </extensions>
</plugin>
