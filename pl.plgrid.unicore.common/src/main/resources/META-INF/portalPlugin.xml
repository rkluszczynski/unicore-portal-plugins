            <implementingClass>pl.plgrid.unicore.portal.core.impl.GridExplorerImpl</implementingClass>
        </ServiceExtension>

        <ServiceExtension>
            <id>gridServicesExplorer</id>
            <interfaceClass>pl.plgrid.unicore.common.GridServicesExplorer</interfaceClass>
            <implementingClass>pl.plgrid.unicore.common.impl.GridServicesExplorerImpl</implementingClass>
        </ServiceExtension>

        <ServiceExtension>
            <id>gridResourcesExplorer</id>
            <interfaceClass>pl.plgrid.unicore.common.GridResourcesExplorer</interfaceClass>
            <implementingClass>pl.plgrid.unicore.common.impl.GridResourcesExplorerImpl</implementingClass>
        </ServiceExtension>


        <MessageSourceExtension>
            <id>pl.plgrid.unicore.portal.uiCommonComponents</id>
            <location>pl.plgrid.unicore.portal.core.i18n.ComponentsI18N</location>
        </MessageSourceExtension>

        <MessageSourceExtension>
            <id>pl.plgrid.unicore.portal.unicoreSiteResources</id>
